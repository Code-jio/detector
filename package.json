{
  "name": "detector-electron",
  "version": "1.0.0",
  "description": "基于Vue3和Three.js的检测器应用",
  "main": "electron/main.js",
  "homepage": "./",
  "scripts": {
    "start": "npm run vr-dev",
    "dev": "cross-env NODE_ENV=development webpack serve --config ./config/webpack.dev.js --progress --color",
    "vr-dev": "node scripts/start-vr-dev.js",
    "urls": "node scripts/show-urls.js",
    "build": "cross-env NODE_ENV=production webpack --config ./config/webpack.prod.js --progress --color",
    "electron": "cross-env NODE_ENV=development electron .",
    "electron-dev": "concurrently \"npm run dev\" \"wait-on https://localhost:3000 --insecure && electron .\"",
    "electron-start": "node scripts/start-electron.js",
    "electron-build": "npm run build && cross-env NODE_ENV=production electron .",
    "dist": "npm run build && electron-builder",
    "dist-win": "npm run build && electron-builder --win",
    "dist-mac": "npm run build && electron-builder --mac",
    "dist-linux": "npm run build && electron-builder --linux"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@types/three": "^0.177.0",
    "@vue/cli-plugin-babel": "^5.0.8",
    "babel-loader": "^9.1.0",
    "concurrently": "^7.6.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "electron": "^25.0.0",
    "electron-builder": "^24.0.0",
    "eslint": "^8.51.0",
    "eslint-plugin-vue": "^9.7.0",
    "eslint-webpack-plugin": "^3.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "image-minimizer-webpack-plugin": "^3.8.0",
    "imagemin": "^8.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^10.0.1",
    "less": "^4.2.0",
    "less-loader": "^11.1.0",
    "mini-css-extract-plugin": "^2.6.1",
    "postcss-loader": "^7.0.1",
    "postcss-preset-env": "^7.8.2",
    "sass": "^1.56.1",
    "sass-loader": "^13.2.0",
    "stylus-loader": "^7.1.0",
    "terser-webpack-plugin": "^5.3.6",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.1",
    "url-loader": "^4.1.1",
    "vue-loader": "^17.0.1",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "^2.7.16",
    "wait-on": "^7.0.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.15.2"
  },
  "dependencies": {
    "@tweenjs/tween.js": "^21.0.0",
    "echarts": "^5.4.3",
    "element-plus": "^2.3.12",
    "mitt": "^3.0.1",
    "stats.js": "^0.17.0",
    "three": "^0.176.0",
    "vue": "^3.2.44",
    "vue-router": "^4.1.6"
  },
  "build": {
    "appId": "com.detector.app",
    "productName": "Detector",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "electron/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "extraResources": [
      {
        "from": "public",
        "to": "public"
      }
    ],
    "win": {
      "target": "nsis",
      "icon": "public/favicon.ico"
    },
    "mac": {
      "target": "dmg",
      "icon": "public/favicon.ico"
    },
    "linux": {
      "target": "AppImage",
      "icon": "public/favicon.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    }
  }
}
